----kpi_1.14(保养台次占比_total)
insert overwrite table  kpi_total
select ----'1.14' as kpi,
 s.mon,
 s.num/t.num as num,
 s.asc_code
from (
select * from kpi_total 
where kpi='kpi_1_1'
  and mon>='${begin_date}'
  and mon<='${end_date}'
 ) s,(
select * from kpi_total 
where kpi='kpi_1'
  and mon>='${begin_date}'
  and mon<='${end_date}'
 ) t 
where s.asc_code=t.asc_code
  and s.mon=t.mon ;

