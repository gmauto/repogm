   second_level_classification,
   case when s.outdate='未知' then '未知'
when s.outdate<'2013-10-01' and mon_diff(t.mon_label,s.outdate)<=23 then '保内'
when s.outdate<'2013-10-01' and mon_diff(t.mon_label,s.outdate)>23 then '保外'
