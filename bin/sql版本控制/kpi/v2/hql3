when mon_diff(t.mon_label,s.outdate)>=36 and mon_diff(t.mon_label,s.outdate)<=47 then '3-4年'
when mon_diff(t.mon_label,s.outdate)>=48 and mon_diff(t.mon_label,s.outdate)<=59 then '4-5年'
